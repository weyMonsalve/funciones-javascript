// Importamos el módulo matematicas.js


// Importamos la función puedeVotar desde index.js
const index = require('./index');


const saludo1 = index.crearSaludo("Andres", "es");
console.log(saludo1);

const saludo2 = index.crearSaludo("Julio", "en");
console.log(saludo2);

const saludo3 = index.crearSaludo("Mauricio", "fr");
console.log(saludo3);


const saludo4 = index.crearSaludo("wbeimar", "jap");
console.log(saludo4)